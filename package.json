{
  "name": "restAPi",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --watch .env --exec \"ts-node src/app.ts\"",
    "dev:nps": "yarn start serve",
    "build": "yarn start build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "husky",
    "presetup": "yarn install",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run --d ./src/database/config/ormconfig.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --d ./src/database/config/ormconfig.ts",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create",
    "seed:run": "ts-node ./node_modules/typeorm/cli.js migration:run --d ./src/database/config/ormconfig-seed.ts",
    "seed:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert --d ./src/database/config/ormconfig-seed.ts",
    "seed:create": "ts-node ./node_modules/.bin/typeorm migration:create"
  },
  "dependencies": {
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "bcrypt": "^6.0.0",
    "chalk": "^5.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.1",
    "express-status-monitor": "^1.3.4",
    "figlet": "^1.9.3",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "microframework-w3tec": "^0.6.3",
    "mongodb": "^6.20.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.15.1",
    "nocache": "^4.0.0",
    "nodemailer": "^7.0.9",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.3",
    "routing-controllers-openapi": "^5.0.0",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.27",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "ncp": "^2.0.0",
    "nodemon": "^3.1.10",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
